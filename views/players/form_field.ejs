<div class="form-group">
  <label for="Manufacturer">Manufacturer</label>


  <input  required value="<%=  Cricket Players List 2021" type="text" pattern="[A-zÀ-ž\s]+"  name="Cricket" id="Cricket" maxlength="25" class="form-control">
</div>


<div class="form-group">
  <label for="Model">Model</label>

  <input  required value="<%= Players.Model %>" type="text" pattern="[A-z0-9À-ž\s]+" name="Model" id="Model"   maxlength="20" class="form-control">
</div>


<div class="form-group">
  <label for="Players">Part Name </label>

  <input  required value="<%= Players.Players %>" type="text" pattern="[A-z0-9À-ž\s]+" name="Part" id="Players"  maxlength="25"  class="form-control">
</div>
 
<div class="form-group">
  <label for="Price">Price</label>
  <input  required value="<%= part.Price %>" type="number" step="0.01" name="Price" id="Price" onKeyPress="if(this.value.length>10) return false;" class="form-control">
</div>




 
<a href="/#products" class="btn btn-danger" >Cancel</a>


 <button class="btn btn-primary"  type="submit" style="background-color: #f3bf3a;"> Save  </button>